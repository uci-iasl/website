{{- $pagesbyname := dict -}}
{{- range where .Site.RegularPages "Section" "people" -}}
  {{- $name := .Params.bibname | default .Params.name -}}
  {{- $fullname := partial "bib/getfullname" $name -}}
  {{- $pagesbyname = $pagesbyname | merge (dict $fullname .) -}}
{{- end -}}
{{- return $pagesbyname -}}
